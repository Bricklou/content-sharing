{
  "rules": {
    "@typescript-eslint/consistent-type-imports": ["error", { "disallowTypeAnnotations": false }],

    "@angular-eslint/directive-selector": [
          "error",
          {
            "type": "attribute",
            "prefix": ["app", "sb"],
            "style": "camelCase"
          }
        ],
        "@angular-eslint/component-selector": [
          "error",
          {
            "type": "element",
            "prefix": ["app", "sb"],
            "style": "kebab-case"
          }
        ]
  }
}
